library(class)
library(gmodels)

data <- read.csv("C:/Users/Admin/Downloads/wisc_bc_data.csv")
data[,2] <- factor(data[,2])
norm <- function(x) (x - min(x)) / (max(x) - min(x))
data_n <- as.data.frame(lapply(data[3:31], norm))

train <- data_n[1:469, ]
test  <- data_n[470:569, ]
pred <- knn(train, test, data[1:469, 2], k=21)

CrossTable(data[470:569, 2], pred, prop.chisq=FALSE)
cat("Accuracy =", mean(pred == data[470:569, 2]) * 100, "%")
